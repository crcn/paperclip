{
  "name": "paperclip-vscode-extension",
  "displayName": "Paperclip",
  "version": "12.0.31",
  "electronVersion": "7.1.10",
  "publisher": "crcn",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/crcn/paperclip.git"
  },
  "main": "lib/client/index.js",
  "engines": {
    "vscode": "^1.41.0"
  },
  "activationEvents": [
    "onLanguage:paperclip",
    "onWebviewPanel:paperclip-preview"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "commands": [
      {
        "command": "paperclip.openPreview",
        "title": "Paperclip: Open Live Preview"
      },
      {
        "command": "paperclip.giveFeedback",
        "title": "Paperclip: Give Feedback"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "paperclip.openPreview",
          "when": "editorLangId == paperclip"
        }
      ]
    },
    "keybindings": [
      {
        "command": "paperclip.openPreview",
        "key": "shift+cmd+v",
        "when": "editorLangId == paperclip"
      }
    ],
    "languages": [
      {
        "id": "paperclip",
        "aliases": [
          "paperclip",
          "paperclip"
        ],
        "extensions": [
          ".pc"
        ],
        "configuration": "./languages/paperclip-html-language-configuration.json"
      },
      {
        "id": "pcss",
        "aliases": [
          "paperclip-css",
          "paperclip-css"
        ],
        "extensions": [
          ".pcss"
        ],
        "configuration": "./languages/paperclip-css-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "paperclip",
        "scopeName": "text.html.paperclip-html",
        "path": "./syntaxes/paperclip-html.tmLanguage.json",
        "embeddedLanguages": {
          "source.pcss": "pcss",
          "source.js": "javascript"
        }
      },
      {
        "language": "pcss",
        "scopeName": "source.pcss",
        "path": "./syntaxes/paperclip-css.json"
      }
    ]
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "prepublishOnly": "npm run build",
    "publish:marketplace": "vsce package && vsce publish"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.1.8",
    "@types/vscode": "^1.41.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/react": "^16.9.20",
    "color": "^3.1.2",
    "lodash": "^4.17.15",
    "paperclip": "^12.0.31",
    "paperclip-utils": "^12.0.30",
    "paperclip-web-renderer": "^12.0.31",
    "vsce": "^1.77.0",
    "vscode-languageclient": "^6.0.1",
    "vscode-languageserver": "^6.0.1",
    "vscode-languageserver-textdocument": "^1.0.0"
  },
  "gitHead": "2ea51225cf122f65f101634b60276a70e667c8d4"
}
